<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.oldbaby.sip.common.mapper.SipPhotoColorMapper">

    <resultMap id="BaseResultMap" type="cn.oldbaby.sip.common.domain.SipPhotoColor">
            <id property="id" column="id" />
            <result property="colorId" column="color_id" />
            <result property="colorName" column="color_name" />
    </resultMap>

    <sql id="Base_Column_List">
        id,color_id,color_name
    </sql>

    <select id="selectByPrimaryKey" parameterType="java.lang.Long" resultMap="BaseResultMap">
        select
        <include refid="Base_Column_List" />
        from sip_photo_color
        where  id = #{id} 
    </select>
    <select id="getList" resultType="cn.oldbaby.sip.common.domain.SipPhotoColor">
        select
        <include refid="Base_Column_List"/>
        from sip_photo_color
        <where>
            <if test="colorId != null and colorId !=''">
                color_id = #{colorId}
            </if>
            <if test="colorName != null and colorName != ''">
                and color_name LIKE CONCAT(#{colorName}, '%')
            </if>
        </where>
    </select>

    <delete id="deleteByPrimaryKey" parameterType="java.lang.Long">
        delete from sip_photo_color
        where  id = #{id} 
    </delete>
    <insert id="insert" keyColumn="id" keyProperty="id" parameterType="cn.oldbaby.sip.common.domain.SipPhotoColor" useGeneratedKeys="true">
        insert into sip_photo_color
        ( id,color_id,color_name)
        values (#{id},#{colorId},#{colorName})
    </insert>
    <insert id="insertSelective" keyColumn="id" keyProperty="id" parameterType="cn.oldbaby.sip.common.domain.SipPhotoColor" useGeneratedKeys="true">
        insert into sip_photo_color
        <trim prefix="(" suffix=")" suffixOverrides=",">
                <if test="id != null">id,</if>
                <if test="colorId != null">color_id,</if>
                <if test="colorName != null">color_name,</if>
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
                <if test="id != null">#{id},</if>
                <if test="colorId != null">#{colorId},</if>
                <if test="colorName != null">#{colorName},</if>
        </trim>
    </insert>
    <update id="updateByPrimaryKeySelective" parameterType="cn.oldbaby.sip.common.domain.SipPhotoColor">
        update sip_photo_color
        <set>
                <if test="colorId != null">
                    color_id = #{colorId},
                </if>
                <if test="colorName != null">
                    color_name = #{colorName},
                </if>
        </set>
        where   id = #{id} 
    </update>
    <update id="updateByPrimaryKey" parameterType="cn.oldbaby.sip.common.domain.SipPhotoColor">
        update sip_photo_color
        set 
            color_id =  #{colorId},
            color_name =  #{colorName}
        where   id = #{id} 
    </update>
</mapper>
